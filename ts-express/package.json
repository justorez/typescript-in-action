{
  "name": "ts-express",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "pnpm build && node ./dist/bin/server.js",
    "watch": "nodemon ./dist/bin/server.js",
    "watch-ts": "nodemon --ext ts --exec ts-node bin/server.ts",
    "build": "tsc && ts-node copyStatic.ts"
  },
  "dependencies": {
    "body-parser": "^1.20.2",
    "cookie-parser": "~1.4.6",
    "debug": "~4.3.4",
    "excel-export": "^0.5.1",
    "express": "~4.18.2",
    "http-errors": "~2.0.0",
    "jade": "~1.11.0",
    "morgan": "~1.10.0",
    "mysql2": "^3.6.1"
  },
  "devDependencies": {
    "@types/body-parser": "^1.19.3",
    "@types/cookie-parser": "^1.4.4",
    "@types/debug": "^4.1.9",
    "@types/express": "^4.17.18",
    "@types/http-errors": "^2.0.2",
    "@types/morgan": "^1.9.6",
    "@types/mysql": "^2.15.22",
    "@types/node": "^18.18.1",
    "@types/shelljs": "^0.8.13",
    "nodemon": "^3.0.1",
    "shelljs": "^0.8.5",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
